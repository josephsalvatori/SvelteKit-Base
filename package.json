		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.0.9",
		"@tailwindcss/vite": "^4.0.9",
		"clsx": "^2.1.1",
		"cookie": "^1.0.2",
		"eslint": "^9.21.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.0.0",
		"postcss": "^8.5.3",
